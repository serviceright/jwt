{
    "name": "serviceright/jwt",
    "description": "A simple library to work with JSON Web Token and JSON Web Signature",
    "type": "library",
    "authors": [
        {
            "name": "Luís Otávio Cobucci Oblonczyk",
            "email": "lcobucci@gmail.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "JWT",
        "JWS"
    ],
    "license": [
        "BSD-3-Clause"
    ],
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/php-invoker": "^3.1",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "~2.3",
        "phpmd/phpmd": "~2.2",
        "mikey179/vfsstream": "~1.6"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\JWT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\JWT\\": [
                "test/unit",
                "test/functional"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
